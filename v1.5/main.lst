....................     if(temp >= 10)  
....................     {  
....................       temp -= 10;  
....................       retval += 0x10;  
....................     }  
....................     else // Get the ones digit by adding the remainder.  
....................     {  
....................       retval += temp;  
....................       break;  
....................     }  
....................   }  
....................  
....................   return(retval);  
.................... }  
